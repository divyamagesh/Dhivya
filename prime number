#include<stdio.h>
#include<conio.h>
void main()
{
    int n,count=0,i; 
    printf("enter the number :");
    scanf("%d",&n);
    if(n==0||n==1)
{
    printf("it is not prime number");
}
    else
    {
        for(i=1;i<=n;i++)
        {
            if(n%i==0)
            {
                count++;
            }
        }
        if(count==2)
                {
                    printf("it is prime number");
                }
                else
                {
                    printf("it is not prime number");
                }
            }
        }
    
